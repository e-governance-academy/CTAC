{#
/**
 * @file
 * Default theme implementation to display an oEmbed resource in an iframe.
 *
 * @ingroup themeable
 */
#}

<!DOCTYPE html>
<html>
<head>
  <style>
    html {
      overflow: hidden;
    }
    iframe {
      min-width: 100%;
      max-width: 100%;
    }
  </style>
</head>
  <body style="margin: 0">
    {{ media|raw }}
  </body>
</html>
